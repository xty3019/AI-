    <script>
        $(function () {
            var UsersID = getQueryString("UsersID");
            GetUserName(UsersID);
        })
        function GetUserName(UsersID) {
            var model = { UsersID: UsersID };
            var Model = { Users: model };
            $.ajax({
                type: "post",
                url: "../../Login/GetLoginData",
                data: JSON.stringify(Model),
                dataType: "json",
                contentType: "application/json",
                success: function (data) {
                    data = eval(data);
                    $.each(data, function (index, data) {
                        $("#UsersName").html(data.UsersName);
                    })
                },
                error: function (cms) {
                }
            });
        }
        function getQueryString(name) {
            var reg = new RegExp('(^|&)' + name + '=([^&]*)(&|$)', 'i');
            var r = window.location.search.substr(1).match(reg);
            if (r != null) {
                return unescape(r[2]);
            }
            return null;
        }
    </script>
